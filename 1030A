#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,a,ans=0;
   vector<int> v;
   cin>>n;
   for(int i=0;i<n;i++){
    cin>>a;
    v.push_back(a);
   }

   vector<int> ::iterator it;
for(it=v.begin();it!=v.end();it++){
    if(*it==1){
        ans=1;
        break;
    }
}
if(ans==1){
    cout<<"HARD"<<endl;
}else{
cout<<"EASY"<<endl;
}


return 0;
}
